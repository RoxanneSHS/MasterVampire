IF
Delay(20)
Range("FarmCenterObject",100)
!Global("CbAnimalGotAway","LOCALS",0)
THEN
RESPONSE #100
SetGlobal("CbAnimalGotAway","LOCALS",0)
ClearActions(Myself)
END

IF
!Range("FarmCenterObject",500)
Global("CbAnimalGotAway","LOCALS",0)
THEN
RESPONSE #100
SetGlobal("CbAnimalGotAway","LOCALS",1)
MoveToObject("FarmCenterObject")
END


